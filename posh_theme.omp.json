{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "console_title": true,
  "console_title_style": "template",
  "console_title_template": "{{if .Root}}<Admin> {{end}} {{.Folder}}",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "#ffffff",
          "properties": {
            "prefix": "",
            "postfix": "",
            "text": "ðŸš€ "
          }
        },
        {
          "type": "session",
          "foreground": "#a277ff",
          "style": "plain",
          "properties": {
            "display_host": false,
            "prefix": "\uf508 "
          }
        },
        {
          "foreground": "#61ffca",
          "properties": {
            "prefix": " \uE5FF ",
            "style": "folder"
          },
          "style": "powerline",
          "type": "path"
        },
        {
          "foreground": "#ffca85",
          "style": "powerline",
          "type": "git",
          "properties": {
            "display_upstream_icon": true,
            "display_branch_status": true
          }
        },
        {
          "type": "executiontime",
          "style": "plain",
          "foreground": "#ff6767",
          "properties": {
            "threshold": 500,
            "style": "austin",
            "prefix": " <#ff6767>\ufbab</> "
          }
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true
}
