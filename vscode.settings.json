{
  "telemetry.enableTelemetry": false,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "terminal.integrated.fontSize": 16,
  "editor.fontFamily": "Rec Mono Duotone, Cascadia Code, Cousine NF, Fire Code, Consolas, 'Courier New', monospace",
  "terminal.integrated.fontFamily": "FiraCode NF Retina",
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "workbench.startupEditor": "none",
  "files.associations": {
    "*.tpl": "html"
  },
  "[javascript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "files.autoSave": "onFocusChange",
  "editor.fontLigatures": "'ss01','ss05','dlig'",
  "prettier.useEditorConfig": false,
  "indentRainbow.colors": [
    "rgba(162,119,255, 0.00)",
    "rgba(162,119,255, 0.05)",
    "rgba(162,119,255, 0.10)",
    "rgba(162,119,255, 0.15)",
    "rgba(162,119,255, 0.20)",
    "rgba(162,119,255, 0.25)",
    "rgba(162,119,255, 0.35)",
    "rgba(162,119,255, 0.40)",
    "rgba(162,119,255, 0.45)",
    "rgba(162,119,255, 0.50)",
    "rgba(162,119,255, 0.55)",
    "rgba(162,119,255, 0.60)",
    "rgba(162,119,255, 0.65)",
    "rgba(162,119,255, 0.70)",
    "rgba(162,119,255, 0.75)",
    "rgba(162,119,255, 0.80)",
    "rgba(162,119,255, 0.85)",
    "rgba(162,119,255, 0.90)",
    "rgba(162,119,255, 0.95)",
    "rgba(162,119,255, 1.00)"
  ],
  "files.eol": "\n",
  "editor.bracketPairColorization.enabled": true,
  "typescript.inlayHints.parameterNames.enabled": "all",
  "[handlebars]": {
    "editor.suggest.insertMode": "replace"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "turboConsoleLog.quote": "'",
  "workbench.colorCustomizations": {
    "editor.selectionBackground": "#3d375ef5",
    "editor.selectionHighlightBackground": "#3d375ef5",
    "editor.findMatchBackground": "#82e2ff30",
    "editor.findMatchHighlightBackground": "#82e2ff30",
    "editorCursor.foreground": "#edecee"
  },
  "debug.console.fontSize": 15,
  "editor.fontSize": 15,
  "editor.linkedEditing": true,
  "workbench.productIconTheme": "bootstrap-product-icons",
  "jest.autoRun": "off",
  "workbench.iconTheme": "Monokai Pro Icons",
  "eslint.format.enable": true,
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.inlineSuggest.enabled": true,
  "explorer.fileNesting.enabled": true,
  "window.confirmBeforeClose": "always",
  "terminal.integrated.profiles.windows": {
    "PowerShell": {
      "source": "PowerShell",
      "icon": "terminal-powershell"
    },
    "Command Prompt": {
      "path": [
        "${env:windir}\\Sysnative\\cmd.exe",
        "${env:windir}\\System32\\cmd.exe"
      ],
      "args": [],
      "icon": "terminal-cmd"
    },
    "Git Bash": {
      "source": "Git Bash"
    },
    "Windows PowerShell": {
      "path": "C:\\WINDOWS\\System32\\WindowsPowerShell\\v1.0\\powershell.exe"
    }
  },
  "terminal.integrated.defaultProfile.windows": "PowerShell",
  "explorer.fileNesting.patterns": {
    "*.ts": "${capture}.js, ${capture}.typegen.ts",
    "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
    "*.jsx": "${capture}.js",
    "*.tsx": "${capture}.ts, ${capture}.typegen.ts",
    "tsconfig.json": "tsconfig.*.json",
    "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml",
    ".env": ".env.*",
    "*.mts": "${capture}.typegen.ts",
    "*.cts": "${capture}.typegen.ts"
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "git.branchProtection": ["master, main, dev, development"],
  "window.experimental.commandCenter": true,
  "editor.formatOnSave": true,
  "workbench.preferredLightColorTheme": "Visual Studio Dark",
  "terminal.integrated.shellIntegration.enabled": true,
  "workbench.colorTheme": "Nu Disco Dark",
  "git.useEditorAsCommitInput": false,
  "editor.cursorBlinking": "phase",
  "svg.preview.mode": "svg",
  "window.commandCenter": true,
  "terminal.integrated.shellIntegration.decorationIcon": "arrow-right",
  "terminal.integrated.shellIntegration.decorationIconSuccess": "arrow-right",
  "terminal.integrated.shellIntegration.decorationIconError": "lightbulb",
  "terminal.integrated.shellIntegration.decorationsEnabled": "overviewRuler",
  "editor.experimental.stickyScroll.enabled": true
}
