{
  "editor.suggestSelection": "first",
  "terminal.integrated.fontSize": 16,
  "editor.fontFamily": "Rec Mono Duotone, Cascadia Code, Cousine NF, Fire Code, Consolas, 'Courier New', monospace",
  "terminal.integrated.fontFamily": "FiraCode NF Retina",
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "workbench.startupEditor": "none",
  "files.associations": {
    "*.tpl": "html"
  },
  "[javascript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "files.autoSave": "onFocusChange",
  "editor.fontLigatures": "'ss01','ss05','dlig'",
  "prettier.useEditorConfig": false,
  "indentRainbow.colors": [
    "rgba(162,119,255, 0.00)",
    "rgba(162,119,255, 0.05)",
    "rgba(162,119,255, 0.10)",
    "rgba(162,119,255, 0.15)",
    "rgba(162,119,255, 0.20)",
    "rgba(162,119,255, 0.25)",
    "rgba(162,119,255, 0.35)",
    "rgba(162,119,255, 0.40)",
    "rgba(162,119,255, 0.45)",
    "rgba(162,119,255, 0.50)",
    "rgba(162,119,255, 0.55)",
    "rgba(162,119,255, 0.60)",
    "rgba(162,119,255, 0.65)",
    "rgba(162,119,255, 0.70)",
    "rgba(162,119,255, 0.75)",
    "rgba(162,119,255, 0.80)",
    "rgba(162,119,255, 0.85)",
    "rgba(162,119,255, 0.90)",
    "rgba(162,119,255, 0.95)",
    "rgba(162,119,255, 1.00)"
  ],
  "files.eol": "\n",
  "editor.bracketPairColorization.enabled": true,
  "typescript.inlayHints.parameterNames.enabled": "all",
  "[handlebars]": {
    "editor.suggest.insertMode": "replace"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "turboConsoleLog.quote": "'",
  "workbench.colorCustomizations": {
    "editor.selectionBackground": "#3d375ef5",
    "editor.selectionHighlightBackground": "#3d375ef5",
    "editor.findMatchBackground": "#82e2ff30",
    "editor.findMatchHighlightBackground": "#82e2ff30",
    "editorCursor.foreground": "#edecee"
  },
  "debug.console.fontSize": 15,
  "editor.fontSize": 15,
  "editor.linkedEditing": true,
  "eslint.format.enable": true,
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.inlineSuggest.enabled": true,
  "explorer.fileNesting.enabled": true,
  "window.confirmBeforeClose": "always",
  "terminal.integrated.profiles.windows": {
    "PowerShell": {
      "source": "PowerShell",
      "icon": "terminal-powershell"
    },
    "Command Prompt": {
      "path": [
        "${env:windir}\\Sysnative\\cmd.exe",
        "${env:windir}\\System32\\cmd.exe"
      ],
      "args": [],
      "icon": "terminal-cmd"
    },
    "Git Bash": {
      "source": "Git Bash"
    },
    "Windows PowerShell": {
      "path": "C:\\WINDOWS\\System32\\WindowsPowerShell\\v1.0\\powershell.exe"
    }
  },
  "terminal.integrated.defaultProfile.windows": "PowerShell",
  "explorer.fileNesting.patterns": {
    "*.ts": "${capture}.js, ${capture}.typegen.ts",
    "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
    "*.jsx": "${capture}.js",
    "*.tsx": "${capture}.ts, ${capture}.typegen.ts",
    "tsconfig.json": "tsconfig.*.json",
    "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml",
    ".env": ".env.*",
    "*.mts": "${capture}.typegen.ts",
    "*.cts": "${capture}.typegen.ts"
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "git.branchProtection": ["master, main, dev, development"],
  "editor.formatOnSave": true,
  "workbench.preferredLightColorTheme": "Visual Studio Dark",
  "terminal.integrated.shellIntegration.enabled": true,
  "git.useEditorAsCommitInput": false,
  "editor.cursorBlinking": "phase",
  "svg.preview.mode": "svg",
  "window.commandCenter": true,
  "terminal.integrated.shellIntegration.decorationsEnabled": "overviewRuler",
  "editor.tabSize": 2,
  "git.enableCommitSigning": true,
  "editor.stickyScroll.enabled": true,
  "editor.stickyScroll.maxLineCount": 10,
  "editor.smoothScrolling": true,
  "debug.javascript.defaultRuntimeExecutable": {
    "pwa-node": "node"
  },
  "tabnine.experimentalAutoImports": true,
  "playwright.reuseBrowser": false,
  "git.mergeEditor": true,
  "editor.suggest.matchOnWordStartOnly": false,
  "svelte.plugin.svelte.note-new-transformation": false,
  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma"
  },
  "svelte.enable-ts-plugin": true,
  "[svelte]": {
    "editor.defaultFormatter": "svelte.svelte-vscode"
  },
  "[svg]": {
    "editor.defaultFormatter": "jock.svg"
  },
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  "workbench.colorTheme": "Nu Disco Dark",
  "vim.easymotion": true,
  "vim.incsearch": true,
  "vim.useSystemClipboard": true,
  "vim.useCtrlKeys": true,
  "vim.hlsearch": true,
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "j"],
      "after": ["<Esc>"]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "d"],
      "after": ["d", "d"]
    },
    {
      "before": ["<C-n>"],
      "commands": [":nohl"]
    },
    {
      "before": ["K"],
      "commands": ["lineBreakInsert"],
      "silent": true
    }
  ],
  "vim.leader": "<space>",
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-f>": false,
    "<C-space>": false
  },
  "editor.lineNumbers": "relative",
  "vim.statusBarColorControl": false,
  "vim.surround": false,
  "vim.sneak": false,
  "vim.highlightedyank.enable": true,
  "vim.highlightedyank.color": "#00837F",
  "workbench.productIconTheme": "bootstrap-product-icons",
  "diffEditor.ignoreTrimWhitespace": false,
  "workbench.iconTheme": "moxer-icons",
  "editor.accessibilitySupport": "off"
}
